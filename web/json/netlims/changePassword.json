{
    "title": "Change Password",
    "style": "width:35%;margin:0 30%",
    "data": [
        {
            "name": "passwordForm",
            "id": "passwordForm",
            "content": [
			                {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "id": "passwordSection",
                    "data": [
                        
                        {
                            "className": "five_sixth",
                            "data": [
                                
                                {
                                    "className": "five_sixth",
                                    "elements": [
										
                                        {
                                            "name": "oldPassword",
                                            "id": "oldPassword",
                                            "type": "password",
											"tabIndex": 1,
                                            "className": "large ",
                                            "title": "Old Password",
                                            "required": "true"
                                        },
                                        {
                                            "name": "newPassword",
                                            "id": "newPassword",
                                            "type": "password",
											"tabIndex": 2,
                                            "className": "large ",
                                            "title": "New Password",
											"required": "true"
                                        },
										 {
                                            "name": "confirmPassword",
                                            "id": "confirmPassword",
                                            "tabIndex": 3,
                                            "type": "password",
                                            "className": "large capital",
                                            "title": "Confirm Password",
                                            "required": true
                                        }

                                    ]
                                }
                            ]
                        }

                    ]
                },


                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "btnSubmit",
                                                "id": "btnSubmit",
                                                "title": "Submit",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1; margin:2px"
                                            },
                                            {
                                                "name": "btnCancel",
                                                "id": "btnCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1; margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}