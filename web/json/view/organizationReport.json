{
    "name": "reportViewForm",
    "id": "reportViewForm",
    "content": [
        {
            "className": "sep-div-view",
            "style": "display:none", 
            "id": "errorDivHeader",
            "data": [
                {
                    "className": "five_sixth",
                    "id": "errorDivvwreportData"
                }
            ]
        },
        {
            "className": "sep-div-view",
            "id": "viewReportHeader",
            "data": [
                {
                    "className": "column-left",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "name": "startMonth",
                                    "id": "startMonth",
                                    "type": "text",
                                    "title": "Start month",
									"style": "width: 60px;"
                                }
                            ]
                        },
                        {
                            "className": "column-right",
                            "elements": [
                                {
                                    "name": "startYear",
                                    "id": "startYear",
                                    "type": "text",
                                    "title": "Start year",
									"style": "width: 60px;"
                                }
                            ]
                        },
                        {
                            "className": "five-sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "endMonth",
                                            "id": "endMonth",
                                            "type": "text",
                                            "title": "End month",
									"style": "width: 60px;"
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "endYear",
                                            "id": "endYear",
                                            "type": "text",
                                            "title": "End year",
											"style": "width: 60px;"
                                        }
                                    ]
                                },
								{
									"className": "column-right",
									"style":"display:none;",
                                    "elements": [
                                        {
                                            "name": "reportName",
                                            "id": "reportName",
                                            "type": "text"
                                        }
                                    ]
								}
                            ]
                        }
                    ]
                },
                {
                    "className": "column-right",
                    "data": [
                        {
                            "className": "five-sixth",
                            "data": [
                                {
                                    "className": "five-sixth",
                                    "id": "netmdlist",
                                    "style": "margin-top:15px"
                                }
                            ]
                        },
                        {
                            "className": "five-sixth",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "showreport",
                                                "id": "showreport",
                                                "title": "Show Report",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1;margin-top:10px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}