{
    "title": "Create NetRX Branch",
    "style": "width:60%;margin:0 20%",
    "data": [
        {
            "name": "newNetRxBranchForm",
            "id": "newNetRxBranchForm",
            "content": [
									    {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivNewNetrxBranchData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "id": "referralsdiv",
                    "data": [
                        {
                            "className": "five_sixth",
                            "data": [
 
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "organizationname",
                                            "id": "organizationname",
                                            "tabIndex": 1,
                                            "type": "text",
                                            "className": "large capital",
                                            "title": "Branch Name",
                                            "required": true
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "organizationaddress",
                                            "id": "organizationaddress",
                                            "type": "textarea",
                                            "className": "large ",
                                            "title": "Address"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "Email",
                                            "id": "Email",
                                            "type": "text",
                                            "className": "large ",
                                            "title": "Email"
											
                                           
                                        }

                                    ]
                                },
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "phone",
                                            "id": "phone",
                                            "type": "text",
											"tabIndex": 2,
                                            "className": "large ",
                                            "title": "Phone",
                                            "required": "true"
                                        }
                                    ]
                                }
                            ]
                        },
						                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "mobile",
                                            "id": "mobile",
                                            "type": "text",
											"tabIndex": 3,
                                            "className": "large ",
                                            "title": "Mobile",
											"required": "true"
											
                                           
                                        }

                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "numberOfDevices",
                                            "id": "numberOfDevices",
                                            "type": "text",
											"tabIndex": 4,
                                            "className": "large ",
                                            "title": "Number Of Devices",
                                            "required": "true"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },

                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "btnNewBranchCreate",
                                                "id": "btnNewBranchCreate",
                                                "title": "Create",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1; margin:2px"
                                            },
                                            {
                                                "name": "btnNewBranchCancel",
                                                "id": "btnNewBranchCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1; margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}