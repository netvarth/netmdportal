{
    "title": "Create NetLims Branch",
    "style": "width:60%;margin:0 20%",
    "data": [
        {
            "name": "newBranchForm",
            "id": "newBranchForm",
            "content": [
                {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivNewNetlimsBranchData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "id": "referralsdiv",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "name": "syncType",
                                    "id": "syncType",
                                    "type": "radio",
                                    "title": "Synchornisation",
                                    "value": [
                                        {
                                            "title": "Enable",
                                            "value": "Enable",
                                            "id": "Enable"
                                        },
                                        {
                                            "title": "Disable",
                                            "value": "Disable",
                                            "id": "Disable",
                                            "checked": true,
                                            "style": "margin-left: 10%;"
                                        }
                                    ]
                                },
                                {
                                    "name": "syncTime",
                                    "id": "syncTime",
                                    "title": "syncTime",
                                    "type": "text",
                                    "className": "small "
                                }
                            ]
                        }
                    ]
					                      
                },
                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "btnNewBranchCreate",
                                                "id": "btnNewBranchCreate",
                                                "title": "Create",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1; margin:2px"
                                            },
                                            {
                                                "name": "btnNewBranchCancel",
                                                "id": "btnNewBranchCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1; margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}