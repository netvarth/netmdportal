{
    "title": "Set Synchronisation",
    "style": "width:60%;margin:0 20%",
    "data": [
        {
            "name": "newsetSyncForm",
            "id": "newsetSyncForm",
            "content": [
                {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivNewNetlimsBranchData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "id": "referralsdiv",
                    "data": [
                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "NetlimsSyncType",
                                            "id": "syncTypeEn",
                                            "type": "radio",
                                            "title": "Synchornisation",
                                            "value": [
                                                {
                                                    "title": "Enable",
                                                    "value": "Enable",
                                                    "id": "netlimsEnable"
                                                },
                                                {
                                                    "title": "Disable",
                                                    "value": "Disable",
                                                    "id": "netlimsDisable",
                                                    "checked": true,
                                                    "style": "margin-left: 20%;"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "id": "syctxtnetlimsEnTime",
                                    "elements": [
                                        {
                                            "name": "NetlimsSyncTime",
                                            "id": "NetlimsSyncTime",
                                            "title": "Syncfreqtype",
                                            "type": "select",
                                            "className": "large "
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "id": "syctxtnetlimsEnInr",
                                    "style": "margin: -9% 30% 0 0;",
                                    "elements": [
                                        {
                                            "name": "NetlimsSyncInterval",
                                            "id": "NetlimsSyncInterval",
                                            "title": "SyncInterval",
                                            "type": "text",
                                            "className": "small "
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                             {
                                                "name": "btnNewNetlimssycEdit",
                                                "id": "btnNewNetlimssycEdit",
                                                "title": "Edit",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1;margin:2px"
                                            },
											 {
                                                "name": "btnNewNetlimsSycCreate",
                                                "id": "btnNewNetlimsSycCreate",
                                                "title": "Done",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1;display:none;margin:2px"
                                            },
                                            {
                                                "name": "btnNewNetlimsCancel",
                                                "id": "btnNewNetlimsCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1;display:none;margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}