{
    "title": "Upload New Version",
    "style": "width:50%;margin:0 20%",
    "data": [
        {
            "name": "newInstaller",
            "id": "newInstaller",
            "content": [
                {
                    "className": "sep-div",
                    "data": [
                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "selectApplication",
                                            "id": "selectApplication",
                                            "type": "combined",
                                            "title": "Application Name",
                                            "items": [
                                                {
                                                    "name": "application",
                                                    "id": "application",
                                                    "type": "select",
                                                    "className": "select",
                                                    "value": [
                                                        {
                                                            "title": "Select Application",
                                                            "id": "select"
                                                        },
                                                        {
                                                            "title": "NetMD"
                                                        },
                                                        {
                                                            "title": "NetLIMS"
                                                        },
                                                        {
                                                            "title": "NetRX"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "versionNo",
                                            "id": "versionNo",
                                            "tabIndex": 3,
                                            "type": "text",
                                            "className": "large capital",
                                            "title": "Version No"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "className": "five_sixth",
                            "data": [
                                {
                                    "className": "column-left",
                                    "id": "warUpload"
                                },
                                {
                                    "className": "column-right",
                                    "id": "queryUpload"
                                }
                            ]
                        }
                    ]
                },
                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "btnNewVersionSave",
                                                "id": "btnNewVersionSave",
                                                "title": "Save",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1; margin:2px"
                                            },
                                            {
                                                "name": "btnNewVersionVancel",
                                                "id": "btnNewVersionCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1; margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}