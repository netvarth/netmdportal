{
    "title": "Order count",
    "style": "width:40%;margin:0 20%",
    "data": [
        {
            "name": "orderCountForm",
            "id": "orderCountForm",
            "content": [
                {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "data": [
                        {
                            "className": "five_sixth",
                            "data": [
 
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "fromDate",
                                            "id": "fromDate",
                                            "tabIndex": 1,
                                            "type": "text",
                                            "className": "large capital",
                                            "title": "From date",
                                            "required": true
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "toDate",
                                            "id": "toDate",
                                            "type": "text",
                                            "className": "large ",
                                            "title": "To date",
                                            "required": true
                                        }
                                    ]
                                }
                            ]
                        }
         
                    ]
                },

                {
                    "className": "sep-div ",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "type": "button",
                                    "content": {
                                        "buttons": [
                                            {
                                                "name": "btnSearch",
                                                "id": "btnSearch",
                                                "title": "Find",
                                                "type": "button",
                                                "className": "stdbtn btn_black",
                                                "style": "opacity:1; margin:2px"
                                            },
                                            {
                                                "name": "btnCancel",
                                                "id": "btnCancel",
                                                "title": "Cancel",
                                                "type": "button",
                                                "className": "stdbtn",
                                                "style": "opacity:1; margin:2px"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                                 {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "orderCount",
                                            "id": "orderCount",
                                            "type": "text",
                                            "className": "large ",
                                            "title": "Order count"
                                           
                                        }
                                    ]
                                }
                    ]
                }
            ]
        }
    ]
}