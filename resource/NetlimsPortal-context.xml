				<entry key="netrxBranch"
					value="com.nv.youNeverWait.util.filter.queryBuilder.NetRxBranchQueryBuilder" />
				<entry key="netrxUser"
					value="com.nv.youNeverWait.util.filter.queryBuilder.NetRxUserQueryBuilder" />
				<entry key="result"
