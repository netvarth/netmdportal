{
    "title": " Create Specimen",
	"style":"width:40%;margin:0 30%",
    "data": [
        {
            "name": "newSpecimen",
            "id": "newSpecimen",
            "content": [
                {
                    "className": "sep-div",
                    "style": "display:none",
                    "id": "errorDivHeader",
                    "data": [
                        {
                            "className": "five_sixth",
                            "id": "errorDivNewSpecimenData"
                        }
                    ]
                },
                {
                    "className": "sep-div",
                    "data": [
                        {
